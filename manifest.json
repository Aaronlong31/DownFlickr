{
  "name": "DownFlickr - Flickr Downloader",
  "version": "0.4.1",
  "description": "Flickr photos downloader extension.",
  "background_page": "background.html",
  "icons": { "16": "icon16.png",
           "48": "icon48.png",
          "128": "icon128.png" },

  "browser_action": {
    "default_icon": "icon16.png",
    "popup": "popup.html"
  },

  "permissions": [
    "http://api.flickr.com/",
    "tabs"
  ]

}